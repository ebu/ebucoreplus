    <dc:contributor xml:lang="en-us">Dalia R. Levine, HBO</dc:contributor>
    <owl:versionInfo xml:lang="en">Create new classes OnStagePosition and Stage and associated
      properties.</owl:versionInfo>
    <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
    <dc:contributor>Masoud Aminipour, not affiliated</dc:contributor>
    <owl:versionInfo xml:lang="en">Version EBU CCDM 2.2</owl:versionInfo>
    <owl:versionInfo xml:lang="en">CCDM is based on EBUCore and brings significant extensions in
      particular in the domain of content consumption.</owl:versionInfo>
    <owl:priorVersion
      rdf:resource="https://www.ebu.ch/metadata/ontologies/ebuccdm/20182011/ebuccdm.rdf"/>
    <owl:versionInfo xml:lang="en">Create Audit Domain classes and associated classes (Rule,
      AuditJob,AuditReport, Measure) and associated propperties.</owl:versionInfo>
    <owl:versionInfo xml:lang="en">Alignment of CCDM wit EBUCore (some EO properties moved up to the
      Asset level).</owl:versionInfo>
    <owl:versionInfo xml:lang="en">Remove formatName and use skos:prefLabel and
      skos:definition.</owl:versionInfo>
    <dc:contributor xml:lang="ch">Jean-Pierre Evain, EBU</dc:contributor>
    <dc:contributor xml:lang="fi">Kim Viljanen, YLE</dc:contributor>
  </rdf:Description>
